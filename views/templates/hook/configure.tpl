<form action="" method="POST">
    <div id='mens' class="error"><h2>{$error}</h2></div>
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4 col-lg-4 col-lg-offset-4 col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1">
                <img src="data:image/png;base64," alt="nube" class="img">


                <label for="name">Usuario</label>
                <div class="form-group">

                    <input type="text" name="name" id="name" placeholder="&#xf007;  Usuario"
                           class="usuario form-control"
                           value="{$nameValue}" required>

                </div>
                <label for="pass">Contraseña</label>
                <div class="form-group">
                    <input type="password" id="pass" name="pass" placeholder="&#xf023;  Contraseña" class="passN"
                           value="{$passValue}" required>
                </div>
                <div class="form-group">
                    <button type="submit" aria-disabled="false" role="button" style="width: 100%; height: 35px"
                            class="ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only btn button otro"
                            name="boton" id='boton' onclick="growl()">Iniciar Sincronización
                    </button>
                </div>
            </div>
        </div>
    </div>
</form>
<script>
    function growl() {
        $.growl.notice({
            title: "",
            size: "large",
            message: "Sincronización en proceso, por favor espere un poco."
        });
    }
</script>